{
  "schemaVersion": 1,
  "id": "${MOD_ID}",
  "version": "${MOD_VERSION}",
  "name": "${MOD_DISPLAY_NAME}",
  "description": "${MOD_DESCRIPTION}",
  "authors": [
    {
      "name": "${MOD_AUTHOR}",
      "contact": {
        "homepage": "${MOD_HOMEPAGE}",
        "issues": "${MOD_ISSUES}",
        "sources": "${MOD_SOURCE}"
      }
    }
  ],
  "icon": "logo.png",

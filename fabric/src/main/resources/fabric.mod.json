  "entrypoints": {
    "main": [
      "${MOD_GROUP}.fabric.${MOD_NAME}Fabric"
    ]
  },
  "mixins": [
    "${MOD_ID}-common.mixins.json"
  ],
  "depends": {
    "minecraft": "~${libs.versions.minecraft.get()}",
